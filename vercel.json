{
  "version": 2,
  "builds": [
    {
      "src": "app-service/frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "app-service/frontend/api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app-service/frontend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app-service/frontend/$1"
    }
  ]
}